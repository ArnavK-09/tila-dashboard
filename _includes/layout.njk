<!doctype html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  {% set description = "Tila Dashboard is a powerful and user-friendly platform that empowers you to monitor your website's uptime, metrics, and incident history. With its sleek design and robust features, you can effortlessly manage your online presence and keep your users informed." %}
  <title>Tila Web Dashboard/Status for '{{ tila.domain }}'</title>
  <link rel="stylesheet" href="global.css">
  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
  <link rel="shortcut icon" href="//fav.farm/ðŸŒ¸" type="image/x-icon">
  <meta name="theme-color" content="#f472b6" />
  <meta property="og:title" content="Tila Web Dashboard/Status for '{{ tila.domain }}'">
  <meta property="og:site_name" content="Tila Web Dashboard/Status for '{{ tila.domain }}'">
  <meta property="og:url" content="https://arnavk-09.github.io/tila-dashboard/">
  <meta property="og:description" content="{{ description }}">
  <meta property="og:type" content="product">
  <meta property="og:image" content="https://fav.farm/ðŸŒ¸">
</head>

<body class="select-none py-6 px-4 bg-brand-950">
  <main class="min-h-full w-full">
    {{ components.navbar() | safe }}
    <section class="pt-10 container full-container p-0 flex flex-col gap-6">
      {{ content | safe }}
    </section>
  </main>
  <script>
    new Chart(document.getElementById('uptimeChart'), {
      type: 'line',
      data: {
        labels: [{% for i in uptimeChartData.labels | list %}"{{ i }}",{% endfor %}],
        datasets: [{
          label: "Uptime",
          data: [{{uptimeChartData.data}}]
        }]
      },
      options: {
        borderColor: "#22c55e",
        responsive: true,
        maintainAspectRatio: true,
        aspectRatio: 20 / 5
      }
    });
  </script>
</body>

</html>